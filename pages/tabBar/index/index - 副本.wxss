/**index.wxss**/
@font-face {
    font-family: 'iconfont';
    src: url('data:font/truetype;charset=utf-8;base64,') format('truetype');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shoucang:before {
  content: "\e613";
}

.icon-qian:before {
  content: "\e671";
}

.icon-gouwuche:before {
  content: "\e664";
}


.banner{
  width:100%;
  height:300rpx;
}
.banner image{
  width:100%;
  height:100%;
}

.sale{
  width:100%;
}
.sale text{
  width:100%;
  font-size:28rpx;
  margin:50rpx 0 30rpx;
  color:#5c5c5c;
  display:block;
  position:relative;
  text-align:center;
}
.sale text::before{
  width:160rpx;
  height:1rpx;
  content: "";
  background:#333;
  position:absolute;
  left:150rpx;
  top:20rpx;
  display:block;
}
.sale text::after{
  width:160rpx;
  height:1rpx;
  content: "";
  background:#333;
  position:absolute;
  right:150rpx;
  top:20rpx;
  display:block;
}

.sale-product{
  display:flex;
}
.pro-left{
  flex:1;
  display:flex;
}
.pro-left image{
  width:150rpx;
  height:150rpx;
  margin-left:16rpx;
}
.pro-right{
  width:70rpx;
  writing-mode: vertical-rl;
  background:#e6e6e6;
  display:flex;
  justify-content: center;
  align-items: center;
  color:#5c5c5c;
}


.list-box{
  width:90%;
}
.list-title {
  color:#5b5b5b;
  font-size:28rpx;
  border-top:solid 1px #e5e3e4;
  margin-top:38rpx;
  padding: 20rpx 0 10rpx;
}
.list-box .list:nth-last-of-type(1) .pro-text{
  border-bottom:none;
}
.img-box{
  font-size:0;
}
.img-box image {
  width:100%;
}
.pro-text{
  display:flex;
  padding: 15rpx 0 15rpx 0;
  border-bottom:solid 1px #e5e4e2;
  margin-bottom:30rpx;
}
.text-lf{
  flex:1;
}
.text-lf>text{
  color:#333;
  font-size:34rpx;
  padding-bottom:12rpx;
  display:block;
}
.text-lf .dw-text{
  display:grid;
  font-size:26rpx;
}

.text-rg{
  display:flex;
  align-items: center;
}
.text-rg .iconfont{
  font-size: 36px;
  color:#5a5a5a;
}
.text-rg .icon-shoucang{
  margin-left:10rpx;
}



.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.list-wrapper-hotel{
  width:100%;
}
.map image{
  width:100%;
  height:260rpx;
}

.address-box{
  padding:20rpx 0;
  border-bottom:solid 1px #e5e4e2;
}
.address-box .title{
  color:#333;
  font-size:40rpx;
  padding-bottom:16rpx;
  display:block;
}
.address-detail{
  display:flex;
  color:#333;
  font-size:24rpx;
}
.address-detail .desc{
  display:grid;
  flex:1;
  position:relative;
}
.address-detail .desc>text{
  line-height:44rpx;
}
.address-detail .desc::after{
  content: '';
  width:2rpx;
  height:114rpx;
  background:#ccc;
  position:absolute;
  right:0;
  top:0;
}
.address-detail .map{
  display:flex;
  align-items: center;
  padding:0 10rpx;
}
.more-hotel{
  padding:30rpx 0 40rpx;
}